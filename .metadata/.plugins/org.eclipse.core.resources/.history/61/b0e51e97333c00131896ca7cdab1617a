package csc7327.tools;

import csc7327.analysis.DataAnalyzer;

/**
 * @author luongnv89
 *
 */
public class Analyzer {
	private static final String tokyoURL = "data/tokyo.txt";
	private static final String amsterdamURL = "data/amsterdam.txt";
	
	static DataAnalyzer dataAnalyzer;
	
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		String dataURL = args[0].equals("tokyo")?tokyoURL:amsterdamURL;
		dataAnalyzer = new DataAnalyzer(dataURL);

	}

}

 * 
 */
package csc7327.objects;

import java.util.Date;

/**
 * The {@link CheckInTime} present the time of checkin
 * @author luongnv89
 *
 */
public class CheckInTime {
	/**
	 * UTC time offset
	 */
	int utcTimeOffset;
	/**
	 * UTC time stamp
	 */
	Date utcTimeStamp;
	/**
	 * @param utcTimeOffset
	 * @param utcTimeStamp
	 */
	public CheckInTime(int utcTimeOffset, Date utcTimeStamp) {
		this.utcTimeOffset = utcTimeOffset;
		this.utcTimeStamp = utcTimeStamp;
	}
	/**
	 * @return the utcTimeOffset
	 */
	public int getUtcTimeOffset() {
		return utcTimeOffset;
	}
	/**
	 * @return the utcTimeStamp
	 */
	public Date getUtcTimeStamp() {
		return utcTimeStamp;
	}
	
}
